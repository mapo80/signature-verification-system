{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "npx --yes openapi-typescript-codegen --input openapi.yaml --output src/api --client axios",
    "test": "vitest run",
    "test:e2e": "npx playwright install --with-deps && npm run build && npx playwright test"
  },
  "dependencies": {
    "antd": "^5.26.6",
    "axios": "^1.11.0",
    "openapi-typescript-codegen": "^0.29.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^7.0.4",
    "vitest": "^1.5.0"
  }
}
